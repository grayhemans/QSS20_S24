   "source": [
    "debar_clean.head()\n",
    "jobs.head()\n",
    "\n",
    "jobs['EMPLOYER_NAME'] = jobs['EMPLOYER_NAME'].astype(str)\n",
    "debar_clean['Name'] = debar_clean['Name'].astype(str)\n",
    "\n",
    "merged_df = jobs.merge(debar_clean, left_on=\"EMPLOYER_NAME\", right_on=\"Name\", how=\"inner\")\n",
    "\n",
    "merged_df"
   ]
  },
